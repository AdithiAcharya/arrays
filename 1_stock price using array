#include<stdio.h>
int main()
{
    int n,i;
    int maxprice=0, count=0;

    pritnf("enter the number of stock prices\n");
    scanf("%d",&n)

    int prices[n];
    printf("enter the stock prices\n");
    for(i=0;i<n;i++)
    {
        scanf("%d",&prices[i]);
    }

    for(i=0;i<n;i++)
    {
        if(prices[i]>maxprice)
        {
            maxprice=price[i];
            count = 1;
        }
        else if(price[i]==maxprice)
        {
            count++
        }
    }
    printf("maximum price is %d",maxprice);
    printf("the count is %d",count);
    return 0;
}
